{
  "name": "origin-ui-bck",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "adddep": "pnpm i && cd docs && pnpm i",
    "cleardep": "rm -rf node_modules && cd docs && rm -rf node_modules",
    "dev": "vite --port=4600",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "format": "./node_modules/.bin/prettier --write 'src/**/*.{js,jsx,ts,tsx,json,css,scss,md}'",
    "docs:dev": "cd docs && pnpm run start",
    "docs:build": "cd docs && pnpm run build"
  },
  "dependencies": {
    "@types/classnames": "^2.3.1",
    "classnames": "^2.3.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.3.0",
    "@commitlint/config-conventional": "^16.2.4",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^1.3.0",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.1",
    "prettier": "^2.6.2",
    "sass": "^1.51.0",
    "typescript": "^4.6.3",
    "vite": "^2.9.9"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  }
}